{
    "version":2,
    "builds":[
        {
            "src": "dist/src/server.js",
            "use": "@vercel/node",
            "config": {"includesFiles": ["dist/**"]}
        }
    ],
    "routes":[
        {
            "src": "routes/(.*)",
            "dest": "dist/src/server.js"
        }
    ]
}